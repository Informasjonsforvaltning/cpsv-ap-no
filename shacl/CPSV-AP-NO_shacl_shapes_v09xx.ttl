##################################

### Property shapes:

## -- adms-properties:

:hasMin0Max1Shape-admsStatus a sh:PropertyShape;
  sh:class skos:Concept;
#  sh:nodeKind sh:IRI;
  sh:maxCount 1;
  sh:message "The property adms:status SHOULD appear only once and MUST be instance of skos:Concept"@en ,
    "Egenskapen adms:status kan ha maks 1 verdi som skal være instans av skos:Concept"@nb ;
  sh:name "status"@en;
  sh:path adms:status;
  sh:severity sh:Warning .

## -- cccev-properties:

:hasMin0MaxNShape-cccevApplicableinPeriodOfTime a sh:PropertyShape;
  sh:class dct:PeriodOfTime;
#  sh:nodeKind sh:IRI;
  sh:path cccev:applicableinPeriodOfTime;
  sh:name "applicable in Period of time"@en, "anvendbarhetsperiode"@nb;
  sh:message "The property cccev:applicableinPeriodOfTime MUST be an IRI referring to an instance of dct:PeriodOfTime"@en,
    "Egenskapen cccev:applicableinPeriodOfTime skal være en IRI som refererer til instans av dct:PeriodOfTime"@nb;
  sh:severity sh:Violation .

:hasMin1Max1Shape-cccevExpectedDataType a sh:PropertyShape;
  sh:nodeKind sh:IRI;
  sh:path cccev:expectedDataType;
  sh:minCount 1;
  sh:maxCount 1;
  sh:name "expected datatype"@en, "forvented datatype"@nb;
  sh:message "The property cccev:expectedDataType MUST have min. 1 and max. 1 value which MUST be an IRI referring to a datatype e.g. xsd:boolean"@en,
    "Egenskapen cccev:expectedDataType skal ha min. 1 og maks. 1 verdi som skal være en IRI som refererer til en datatype som f.eks. xsd:boolean"@nb;
  sh:severity sh:Violation .

:hasMin0MaxNShape-cccevExpectedValue a sh:PropertyShape;
  sh:nodeKind sh:Literal;
  sh:path cccev:expectedValue;
  sh:name "expected value"@en, "forventet verdi"@nb;
  sh:message "The property cccev:expectedValue MUST be a Literal"@en,
    "Egenskapen cccev:expectedValue skal være en Literal"@nb;
  sh:severity sh:Violation .

:hasMin0MaxNShape-cccevIsSupportedBy a sh:PropertyShape;
  sh:class cccev:DocumentReference;
#  sh:nodeKind sh:IRI;
  sh:path cccev:isSupportedBy;
  sh:name "is supported by"@en, "bekreftes ved"@nb;
  sh:message "The property cccev:isSupportedBy MUST be an IRI referring to an instance of cccev:DocumentReference"@en,
    "Egenskapen cccev:isSupportedBy skal være en IRI som refererer til instans av cccev:DocumentReference"@nb;
  sh:severity sh:Violation .

:hasMin0MaxNShape-cccevLevelOfCertification a sh:PropertyShape;
  sh:class skos:Concept;
#  sh:nodeKind sh:IRI;
  sh:path cccev:levelOfCertification;
  sh:name "level of certification"@en, "sertifiseringsnivå"@nb;
  sh:message "The property cccev:levelOfCertification MUST be an IRI referring to an instance of skos:Concept"@en,
    "Egenskapen cccev:levelOfCertification skal være en IRI som refererer til instans av skos:Concept"@nb;
  sh:severity sh:Violation .

:hasMin0MaxNShape-cccevMaximumValue a sh:PropertyShape;
  sh:nodeKind sh:Literal;
  sh:path cccev:expectedValue;
  sh:name "maximum value"@en, "øvre verdigrense"@nb;
  sh:message "The property cccev:maximumValue MUST be a Literal"@en,
    "Egenskapen cccev:maximumValue skal være en Literal"@nb;
  sh:severity sh:Violation .

:hasMin0MaxNShape-cccevMetByEvidence a sh:PropertyShape;
  sh:class cv:Evidence;
#  sh:nodeKind sh:IRI;
  sh:path cccev:metByEvidence;
  sh:name "met by Evidence"@en, "oppfylt med dokumentasjon"@nb;
  sh:message "The property cccev:metByEvidence MUST be an IRI referring to an instance of cv:Evidence"@en,
    "Egenskapen cccev:metByEvidence skal være en IRI som refererer til instans av cv:Evidence"@nb;
  sh:severity sh:Violation .

:hasMin0MaxNShape-cccevMinimumValue a sh:PropertyShape;
  sh:nodeKind sh:Literal;
  sh:path cccev:expectedValue;
  sh:name "minimum value"@en, "nedre verdigrense"@nb;
  sh:message "The property cccev:minimumValue MUST be a Literal"@en,
    "Egenskapen cccev:minimumValue skal være en Literal"@nb;
  sh:severity sh:Violation .

:hasMin0MaxNShape-cccevTypeOfCopyQuality a sh:PropertyShape;
  sh:class skos:Concept;
#  sh:nodeKind sh:IRI;
  sh:path cccev:levelOfCertification;
  sh:name "type of copy quality"@en, "type kopieringskvalitet"@nb;
  sh:message "The property cccev:typeOfCopyQuality MUST be an IRI referring to an instance of skos:Concept"@en,
    "Egenskapen cccev:typeOfCopyQuality skal være en IRI som refererer til instans av skos:Concept"@nb;
  sh:severity sh:Violation .

:hasMin0MaxNShape-cccevTypeOfTranslation a sh:PropertyShape;
  sh:class skos:Concept;
#  sh:nodeKind sh:IRI;
  sh:path cccev:typeOfTranslation;
  sh:name "type of translation"@en, "type oversettelse"@nb;
  sh:message "The property cccev:typeOfTranslation MUST be an IRI referring to an instance of skos:Concept"@en,
    "Egenskapen cccev:typeOfTranslation skal være en IRI som refererer til instans av skos:Concept"@nb;
  sh:severity sh:Violation .

:hasMin0MaxNShape-cccevUrl a sh:PropertyShape;
  sh:nodeKind sh:IRI;
  sh:path cccev:url;
  sh:name "url"@en, "URL"@nb;
  sh:message "The property cccev:url MUST be an IRI"@en,
    "Egenskapen cccev:url skal være en IRI"@nb;
  sh:severity sh:Violation .

## -- cpsv-properties:

:hasMin0MaxNShape-cpsvFollows a sh:PropertyShape;
  sh:class cpsv:Rule;
#  sh:nodeKind sh:IRI;
