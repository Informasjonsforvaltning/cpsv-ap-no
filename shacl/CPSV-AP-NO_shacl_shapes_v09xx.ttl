### Klassen Katalog (dcat:Catalog)
:CatalogShape a sh:NodeShape;
  sh:name "Catalog"@en, "Katalog"@nb;
  sh:property
   # mandatory
      :hasMin1MaxNShape-dctDescription, # beskrivelse
      :hasMin1Max1Shape-dctIdentifier, # identifikator
      :hasMin1MaxNShape-dctTitle, #tittel
      :hasMin1MaxNShape-dcatnoContainsService, # inneholder tjeneste
      :hasMin1Max1Shape-dctPublisher, # utgiver
   # recommended
      :hasMin0MaxNShape-dctSpatial, # dekningsområde
      :hasMin0MaxNShape-dctModified, # endringsdato
#      :hasMin0Max1Shape-dctAccrualPeriodicity, # frekvens
      :HasShapeCV-dctAccrualPeriodicityMin0Max1, # frekvens kontrollert vokabular
      :hasMin0Max1Shape-foafHomepage, # hjemmeside
#      :hasMin0Max1Shape-dctLicense, # lisens
      :HasShapeCV-dctLicenseMin0Max1, # lisens kontrollert vokabular
      :hasMin0MaxNShape-dctProvenance, # opphav
#      :hasMin0MaxNShape-dctLanguage, # språk
      :HasShapeCV-dctLanguageMin0MaxN, # kontrollert vokabular for språk
      :hasMin0MaxNShape-dcatThemeTaxonomy, # temaer
      :hasMin0Max1Shape-dctIssued, # utgivelsesdato
   # optional
      :hasMin0Max1Shape-dctIsPartOfCatalog, # er del av
      :hasMin0MaxNShape-dctHasPartCatalog, # har del
      :hasMin0MaxNShape-dcatnoContainsEvent, # inneholder hendelse
      :hasMin0Max1Shape-dctCreator, # produsent
      :hasMin0MaxNShape-dctRights # rettigheter
      ;
   sh:targetClass dcat:Catalog .

