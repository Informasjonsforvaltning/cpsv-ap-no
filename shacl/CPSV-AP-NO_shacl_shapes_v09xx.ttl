### Klassen Hendelse (cv:Event), Livshendelse (cv:LifeEvent), Virksomhetshendelse (cv:BusinessEvent)
:EventShape a sh:NodeShape;
  sh:name "Event, Live Event, Business Event"@en, "Hendelse, Livshendelse, Virksomhetshendelse"@nb;
  sh:property
    # mandatory properties:
      :hasMin1Max1Shape-dctIdentifier, # identifikator
      :hasMin1MaxNShape-dctTitle, # navn
    # recommended properties:
      :hasMin0MaxNShape-dctDescription, # beskrivelse
    # optional properties:
      :hasMin0MaxNShape-dctSubject, # begrep
      :hasMin0MaxNShape-dcatDistribution, # distribusjon
      :hasMin0MaxNShape-dctType # type
    ;
  sh:or (
    [sh:targetClass cv:Event]
    [sh:targetClass cv:LifeEvent]
    [sh:targetClass cv:BusinessEvent] ) .

