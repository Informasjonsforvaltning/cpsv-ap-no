### Klassen Offentlig tjeneste  (cpsv:PublicServicve)
# det som er i tillegg til Tjeneste (cpsvno:Servicve)
:PublicServiceShape a sh:NodeShape;
  sh:name "Public Service"@en , "Offentlig tjeneste"@nb ;
  sh:property
    # mandatory properties
      :hasMin1MaxNShape-dctDescription, # beskrivelse
      :hasMin1Max1Shape-cvHasCompetentAuthority, # har kompetent organ
      :hasMin1Max1Shape-dctIdentifier, # identifikator
      :hasMin1MaxNShape-dcatContactPoint, # kontaktpunkt
      :hasMin1MaxNShape-dctTitle, # navn
      :hasMin1MaxNShape-cpsvProduces, # produserer
    # recommended properties
      :hasMin0MaxNShape-dctSubject, # begrep
      :hasMin0MaxNShape-dctSpatial, # dekningsområde
      :hasMin0MaxNShape-cpsvHasInput, # dokumentasjonskrav
      :hasMin0MaxnShape-dctIsPartOfService, # er del av
      :hasMin0MaxnShape-dctHasPartService, # har del
      :hasMin0MaxNShape-foafHomepage, # hjemmeside
      :hasMin0Max1Shape-admsStatus, # status
      :hasMin0MaxNShape-cvThematicArea, # temaområde
      :hasMin0MaxNShape-dctType, # type
    # optional properties:
      :hasMin0Max1Shape-cvProcessingTime, # behandlingstid
      :hasMin0MaxNShape-cvIsDescribedAt, # beskrivende datasett
      :hasMin0MaxNShape-cvHasParticipation, # deltagende
      :hasMin0MaxNShape-cvIsGroupedBy, # er gruppert ved
      :hasMin0MaxNShape-cvIsClassifiedBy, # er klassifisert under
      :hasMin0MaxNShape-cvHasChannel, # er tilgjengelig via
      :hasMin0MaxNShape-cpsvFollows, # følger regel
      :hasMin0MaxNShape-cvHasCost, # gebyr
      :hasMin0MaxNShape-dctRequires, # krever
      :hasMin0MaxNShape-cvSector, # næringsgruppering
      :hasMin0MaxNShape-dcatKeyword, # nøkkelord
      :hasMin0MaxNShape-cvHasLegalResource, # relatert regelverk
      :hasMin0MaxNShape-dctRelationService, # relatert tjeneste
      :hasMin0MaxNShape-dctLanguage, # språk
      :hasMin0MaxNShape-cvHasCriterion # vilkår
  ;
  sh:targetClass cpsv:PublicService .

