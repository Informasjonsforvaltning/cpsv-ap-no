### --- Class shapes:

### Klassen AdministrativEnhet
### @@@ Sjekk om EU kommer til å endre denne
:AdminUnitShape a sh:NodeShape;
   sh:name "Administrative Unit"@en, "Administrativ enhet"@nb ;
   sh:property
      # mandatory properties:
         :hasMin1Max1Shape-cvCode, # kode, cv:code
         :hasMin1Max1Shape-cvLevel # nivå, cv:level
      ;
   sh:targetClass cv:AdminUnit .

### Klassen Adresse:
### @@@ Sjekk om EU kommer til å endre på denne klassen.
:AddressShape a sh:NodeShape;
   sh:name "Address"@en, "Adresse"@nb;
   sh:property
      # optional properties:
         :hasMin0Max1Shape-cvAdminUnit, # administrativ enhet, cv:adminUnit
         :hasMin0MaxNShape-locnAdminUnitL1, # admin enhet nivå 1, cv:adminUnitL1
         :hasMin0MaxNShape-locnAdminUnitL2, # admin enhet nivå 2, cv:adminUnitL2
         :hasMin0Max1Shape-locnAddressId, # adresseId, locn:adressID
         :hasMin0MaxNShape-locnAddressArea, # adresseringsområde, locn:addressArea
         :hasMin0MaxNShape-locnLocatorName, # adressetilleggsnavn, locn:locatorName
         :hasMin0Max1Shape-locnLocatorDesignator, # @@@ hus-/oppgangsnr, locn:locatorDesignator
         :hasMin0MaxNShape-locnFullAddress, # fullstendig adresse, locn:fullAddress
         :hasMin0MaxNShape-locnThroughfare, # gate-/vei-/omraådenavn, locn:throughfare
         :hasMin0Max1Shape-locnPoBox, # postboks, locn:poBox
         :hasMin0Max1Shape-locnPostCode, # postnummer, locn:postCode
         :hasMin0Max1Shape-locnPostName  # poststed, locn:postName
      ;
   sh:targetClass locn:Address .

### Klassen Aktør (foaf:Agent), Organisasjon (org:Organization) og Offentlig organisasjon (cv:PublicOrganisation)
### cv:PublicOrganisation rdfs:subClassOf org:Organization .
### org:Organization rdfs:subClassOf foaf:Agent.
:AgentShape a sh:NodeShape;
  sh:name "Agent"@en, "Aktør"@nb;
  sh:property
    # mandatory properties:
      :hasMin1Max1Shape-dctIdentifier , # identifikator (dct:identifier)
      :hasMin1MaxNShape-dctTitle, # navn (dt:title)
    # recommended properties:
      :hasShapeCV-dctTypeAgentMin0Max1, # type (dct:type), kontrollert vokabular
    # optional properties:
      :hasMin0MaxNShape-locnAddress, # adresse (locn:address)
      :hasMin0MaxNShape-cvPlaysRole # har rolle i (cv:playsRole)
    ;
 sh:and ( [sh:targetClass foaf:Agent, org:Organization, cv:PublicOrganisation ]
          [ sh:not ( [sh:targetObjectsOf cv:role]) ] );
   .

### Klassen Begrensning (cv:Constraint)
:ConstraintShape a sh:NodeShape;
   sh:name "Constraint"@en, "Begrensning"@nb;
   sh:property
      # mandatory:
         :hasMin1Max1Shape-cvConstrains, # begrenser, cv:constrains
         :hasMin1Max1Shape-dctIdentifier, # identifikator, dct:identifier
         :hasMin1MaxNShape-dctTitle, # navn, dct:title
      # recommended:
         :hasMin0MaxNShape-dctDescription, # beskrivelse, dct:description
      # optional:
         :hasMin0MaxNShape-cvIsRequirementOf, # er subkrav av, cv:isRequirementOf
         :hasMin0MaxNShape-cvIsDerivedFrom, # er utledet av, cv:isDerivdFrom
         :hasMin0Max1Shape-dctPublisher, # er utstedt av, dct:publisher
         :hasMin0MaxNShape-cvHasEvidenceTypeList, # har dokumentasjonslitestype, cv:hasEvidenceTypeList
         :hasMin0MaxNShape-cvHasQualifiedRelation, # har kvalifisert relasjon til andre krav, cv:hasQualifiedRelation
         :hasMin0MaxNShape-cvHasRequirement, # har mer spesifikt krav, cv:hasRequirement
         :hasMin0MaxNShape-cvHasSupportingEvidence, # har understøttende dokumentajson, cv:hasSupportingEvidence
         :hasMin0MaxNShape-cvFulfils, # tilfredsstiller regel, cv:fulfils
         :hasMin0MaxNShape-dctType # type, dct:type
      ;
   sh:targetClass cv:Constraint .

