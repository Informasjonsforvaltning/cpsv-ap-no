  sh:message "The property cv:value may have maks. 1 value which should be xsd:decimal"@en, "Egenskapen cv:value kan ha maks. 1 verdi som bør være xsd:decimal"@nb;
  sh:name "value"@en, "beløp"@nb;
  sh:datatype xsd:decimal;
  sh:path cv:value;
  sh:severity sh:Violation .


## -- dcat-properties:

:hasMin1MaxNShape-dcatContactPoint a sh:PropertyShape;
  sh:name "contact point"@en, "kontaktpunkt"@nb;
  sh:message "The property dcat:contactPoint MUST have min. 1 value referring to an instance of vcard:Kind"@en,
    "Egenskapen dcat:contactPoint skal ha minst 1 verdi som refererer til instans av vcard:Kind"@nb;
  sh:class vcard:Kind;
  sh:path dcat:contactPoint;
  sh:severity sh:Violation .

:hasMin0MaxNShape-dcatDistribution a sh:PropertyShape;
  sh:name "distribution"@en, "distribusjon"@nb;
  sh:message "The property dcat:distribution MUST refer to an instance of dcat:Distribution"@en,
    "Egenskapen dcat:distribution skal referere til instans av dcat:Distribution"@nb;
  sh:class dcat:Distribution;
  sh:path dcat:distribution;
  sh:severity sh:Violation .

:hasMin0Max1Shape-dcatEndDate a sh:PropertyShape;
  sh:name "end date"@en, "sluttdato/-tid"@nb;
  sh:message "The property dcat:endDate may have max. 1 value which MUST be either xsd:date or xsd:dateTime"@en,
    "Egenskapen dcat:endDate kan ha maks. 1 verdi som skal være enten xsd:date eller xsd:dateTime"@nb;
  sh:path dcat:endDate;
  sh:or ([
          sh:datatype xsd:date
      ]
      [
          sh:datatype xsd:dateTime
      ]
  ) ;
  sh:severity sh:Violation .

:hasMin0MaxNShape-dcatKeyword a sh:PropertyShape;
  sh:name "keyword"@en;
  sh:nodeKind sh:Literal;
  sh:path dcat:keyword;
  sh:severity sh:Violation .

# denne brukes ikke?
