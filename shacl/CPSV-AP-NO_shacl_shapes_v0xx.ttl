  sh:minCount 1;
  sh:name "landingPage"@en;
  sh:nodeKind sh:IRI;
  sh:path dcat:landingPage;
  sh:severity sh:Warning .

:hasMin0Max1Shape-dcatStartDate a sh:PropertyShape;
  sh:name "start date"@en, "startdato/-tid"@nb;
  sh:message "The property dcat:startDate may have max. 1 value which MUST be either xsd:date or xsd:dateTime"@en,
    "Egenskapen dcat:startDate kan ha maks. 1 verdi som skal være enten xsd:date eller xsd:dateTime"@nb;
  sh:path dcat:startDate;
  sh:or ([
          sh:datatype xsd:date
      ]
      [
          sh:datatype xsd:dateTime
      ]
  ) ;
  sh:severity sh:Violation .

:hasMin0MaxNShape-dcatThemeTaxonomy a sh:PropertyShape;
  sh:name "theme taxonomy"@en, "temaer"@nb;
  sh:nodeKind sh:IRI;
  sh:message "The property dct:type UST be an IRI (referring to e.g. a controlled vocabulary)"@en,
