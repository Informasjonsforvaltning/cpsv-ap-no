  sh:nodeKind sh:IRI;
  sh:path cv:hasCriterion;
  sh:severity sh:Violation .

:hasMin0MaxNShape-cvHasLegalResource a sh:PropertyShape;
  sh:class eli:LegalResource;
  sh:name "has legal resource"@en, "relatert regelverk"@nb;
  sh:message "The property cv:hasLegalResource MUST be an IRI referring to an instance of eli:LegalResource"@en,
    "Egenskapen cv:hasLegalResource skal være en IRI som refererer til instans av cv:hasLegalResource"@nb;
  sh:nodeKind sh:IRI;
  sh:path cv:hasLegalResource;
  sh:severity sh:Violation .

:hasMin0MaxNShape-cvHasParticipation a sh:PropertyShape;
  sh:class cv:Participation;
  sh:name "has participation"@en, "deltagende"@nb;
  sh:message "The property cv:hasParticipation MUST be IRI referring to an instance of cv:Participation"@en,
    "Egenskapen cv:hasParticipation skal være IRI som refererer til instans av cv:Participation"@nb;
  sh:nodeKind sh:IRI;
  sh:path cv:hasParticipation;
  sh:severity sh:Violation .

:hasMin0Max1Shape-cvIfAccessedThrough a sh:PropertyShape;
  sh:class cv:Channel;
  sh:maxCount 1;
  sh:message "The property cv:ifAccessedThrough may have max. 1 value which MUST be instance of cv:Channel"@en,
    "Egenskapen cv:ifAccessedThrough kan ha maks. 1 verdi som skal referere til instans av cv:Channel";
  sh:name "if accessed through"@en, "hvis tilbys gjennom"@nb;
  sh:nodeKind sh:IRI;
  sh:path cv:ifAccessedThrough;
  sh:severity sh:Violation .

:hasMin0MaxNShape-cvIsDefinedBy a sh:PropertyShape;
  sh:or (
    [sh:class cv:PublicOrganisation]
    [sh:class foaf:Agent] );
  sh:name "isDefinedBy"@en, "er bestemt av"@nb;
  sh:message "The property cv:isDefinedBy MUST be IRI referring to an instance of cv:PublicOrganisation or foaf:Agent"@en,
    "Egenskapen cv:isDefinedBy skal være en IRI som refererer til instans av cv:PublicOrganisation eller foaf:Agent"@nb;
  sh:nodeKind sh:IRI;
  sh:path cv:isDefinedBy;
  sh:severity sh:Violation .

:hasMin0MaxNShape-cvIsClassifiedBy a sh:PropertyShape;
  sh:class skos:Concept;
  sh:name "is classified by"@en, "er klassifisert ved"@nb;
  sh:message "The property cv:isClassifiedBy MUST be IRI referring to a defined concept"@en,
    "Egenskapen cv:isClassifiedBy skal være IRI (som refererer til begrep)"@nb;
  sh:nodeKind sh:IRI;
  sh:path cv:isClassifiedBy;
  sh:severity sh:Warning .

:hasMin0MaxNShape-cvIsDescribedAt a sh:PropertyShape;
  sh:class dcat:Dataset;
  sh:name "is described at"@en, "beskrivende datasett"@nb;
  sh:message "The propety cv:isDescribedAt MUST be IRI referring to an instace of dcat:Dataset"@en,
    "Egenskapen cv:isDescribedAt skal være IRI som refererer til instans av dcat:Dataset"@nb;
  sh:nodeKind sh:IRI;
  sh:path cv:isDescribedAt;
  sh:severity sh:Violation .

:hasMin0MaxNShape-cvIsGroupedBy a sh:PropertyShape;
  sh:class cv:Event;
  sh:name "is grouped by"@en, "er gruppert ved"@nb;
  sh:message "The property cv:IsGroupedBy MUST be IRI which MUST refer to an instance of cv:Event"@en,
    "Egenskapen cv:IsGroupedBy skal være IRI som refererer til instans av cv:Event"@nb;
  sh:nodeKind sh:IRI;
  sh:path cv:IsGroupedBy;
  sh:severity sh:Violation .

:hasMin0MaxNShape-cvOwnedBy a sh:PropertyShape;
  sh:class cv:PublicOrganisation;
  sh:name "owned by"@en, "eies av"@nb;
  sh:message "The property cv:ownedBy MUST be an IRI referring to an instance of foaf:Agent or cv:PublicOrganisation"@en,
    "Egenskapen cv:ownedBy skal være IRI som refererer til instans av foaf:Agent eller cv:PublicOrganisation"@nb;
  sh:nodeKind sh:IRI;
  sh:path cv:ownedBy;
  sh:severity sh:Violation .

:hasMin0MaxNShape-cvPlaysRole a sh:PropertyShape;
  sh:class cv:Participation;
  sh:name "plays role"@en, "har rolle i"@nb;
  sh:message "The property cv:playsRole MUST be an IRI referring to an instance of cv:Participation"@en,
    "Egenskapen cv:playsRole skal være en IRI som refererer til instans av cv:Participation"@nb;
  sh:nodeKind sh:IRI;
  sh:path cv:playsRole;
  sh:severity sh:Violation .

:hasMin0MaxNShape-cvProcessingTime a sh:PropertyShape;
  sh:name "processing time"@en, "behandlingstid";
  sh:message "The property cv:processingTime MUST be xsd:duration"@en,
    "Egenskapen cv:processingTime skal være verdi av type xsd:duration"@nb;
  sh:datatype xsd:duration;
  sh:path cv:processingTime;
  sh:severity sh:Violation .

:hasMin1MaxNShape-cvRole a sh:PropertyShape;
  sh:class skos:Concept;
  sh:message "The property cv:role MUST be an IRI referring to an instance of skos:Concept"@en,
    "Egenskapen cv:role skal være en IRI som refererer til instans av skos:Concept"@nb;
  sh:minCount 1;
  sh:name "role"@en;
  sh:nodeKind sh:IRI;
  sh:path cv:role;
  sh:severity sh:Warning .

:hasMin0MaxNShape-cvSector a sh:PropertyShape;
  sh:class skos:Concept;
  sh:name "sector"@en, "næringsgruppering"@nb;
  sh:message "The property cv:sector MUST be IRI referring to concept"@en,
    "Egenskapen cv:sector skal være IRI som refererer til begrep"@nb;
  sh:nodeKind sh:IRI;
  sh:path cv:sector;
  sh:severity sh:Warning .

:hasMin0MaxNShape-cvThematicArea a sh:PropertyShape;
  sh:class skos:Concept;
  sh:name "thematic area"@en, "temaområde";
  sh:nodeKind sh:IRI;
  sh:path cv:thematicArea;
  sh:severity sh:Warning .

:hasMin0Max1Shape-cvValue a sh:PropertyShape;
