  sh:nodeKind sh:IRI;
  sh:path cv:hasCost;
  sh:severity sh:Violation .

:hasMin0MaxNShape-cvHasCriterion a sh:PropertyShape;
  sh:class cv:CriterionRequirement;
  sh:name "has criterion"@en, "vilkår"@nb;
  sh:message "The property cv:hasCriterion MUST be IRI referring to an instance of cv:CriterionRequirement"@en,
