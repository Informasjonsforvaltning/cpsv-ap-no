  sh:nodeKind sh:IRI;
  sh:path cv:hasChannel;
  sh:severity sh:Violation .

:hasMin0MaxNShape-cvHasCost a sh:PropertyShape;
  sh:class cv:Cost;
  sh:name "has cost"@en, "gebyr"@nb;
  sh:message "The property cv:hasCost MUST refer to an instance of cv:Cost"@en,
